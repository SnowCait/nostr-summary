{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "nostr-summary",
	"main": "src/index.ts",
	"compatibility_date": "2025-10-11",
	"observability": {
		"enabled": true
	},
	"triggers": {
		"crons": ["0 * * * *"]
	},
	"kv_namespaces": [
		{
			"binding": "KV",
			"id": "%KV_ID%"
		}
	]
}
